{
  "manifest_version": 2,
  "name": "ConverType",
  "version": "1.0",
  "description": "ConverType",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "./img/logo.png",
    "default_popup": "./html/popup.html"
  }
}
